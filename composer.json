{
  "description": "Parse and format to LCOV your code coverage reports. The best way to share your code coverage stats.",
  "homepage": "https://dev.belin.io/lcov.php",
  "license": "MIT",
  "name": "cedx/lcov",
  "type": "library",
  "authors": [{
    "email": "cedric@belin.io",
    "name": "CÃ©dric Belin"
  }],
  "autoload": {
    "psr-4": {"Lcov\\": "lib/"}
  },
  "autoload-dev": {
    "files": ["vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"],
    "psr-4": {"Lcov\\": "test/"}
  },
  "config": {
    "optimize-autoloader": true
  },
  "keywords": [
    "coverage",
    "formatter",
    "lcov",
    "parser",
    "test"
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pcre": "*",
    "ext-spl": "*"
  },
  "require-dev": {
    "consolidation/robo": "^1.3.3",
    "henrikbjorn/lurker": "^1.2.0",
    "phpstan/phpstan": "^0.10.7",
    "phpunit/phpunit": "^7.5.1"
  },
  "scripts": {
    "coverage": "robo coverage",
    "test": "robo test"
  },
  "support": {
    "docs": "https://dev.belin.io/lcov.php/api",
    "issues": "https://github.com/cedx/lcov.php/issues"
  }
}
